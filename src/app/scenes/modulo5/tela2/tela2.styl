.tela.mod5tela2
  position relative
  min-height 100%
  width 100%
  height 100% /* cross-browser */
  /*background url('../assets/images/px/formas/grafismos.png')
  background-attachment fixed
  background-position left top
  background-repeat no-repeat
  background-size auto 100%
  background-color white*/
  overflow hidden

  .mod5tela1-bg
    position fixed
    width 100%
    height 100%
    //background url('../assets/images/px/bgs/bg_01.png')
    //background-size cover
    //background-position right top
    //background-repeat no-repeat
    //background-attachment fixed
    background-color white

  .content-desktop
    display block

  .content-mobile
    display none

  .container-fluid
    height 100%
    padding 0
    opacity 0

    &.started
      opacity 1
      animation 0.5s fadeIn

    .page-menu
      position absolute
      right 20px
      top 50%
      transform translateY(-50%)
      width 40px
      background rgba(0,0,0,0.5)
      border-radius 20px
      padding-top 10px
      padding-bottom 0px
      z-index 2000

      .menu-nav
        position relative
        width 20px
        height 20px
        border-radius 50%
        background transparent
        border 2px solid white
        left 10px
        margin-bottom 10px

        &.current
          background white


    .page0
      position absolute
      left 0
      top 0
      width 100%
      height 100%
      background url('../assets/images/px/bg/bg01.png')
      background-size cover
      background-position center
      background-repeat no-repeat
      z-index 1000
      opacity 1

      &.ng-hide-add
        animation 0.5s fadeOut



    .bigtext
      font-size 1.3rem
      line-height 110%
    .semititle
      font-size 1.6rem
      line-height 110%
    .subtitle
      font-size 2rem
      line-height 110%
    .title
      font-size 3rem
      line-height 110%

    .black
      color black
    .white
      color white
    .blue
      color #013469

    .strong
      font-weight 200
    .semibold
      font-weight 500
    .bold
      font-weight 900


    .center
      text-align center

    .boxed
      padding 1rem
      display block
      line-height 120%

      &.max-box-30
        width 30%

      &.max-box-60
        width 60%

      &.bg-black-50
        background rgba(0,0,0,0.5)

      &.bg-white
        background white

      &.bg-blue
        background #013469

      &.bg-purple
        background linear-gradient(90deg, #522e91, #a53cbf)

      &.bg-blue-gradient
        background linear-gradient(90deg, #012f5e, #094cab)

      &.footer
        position absolute
        bottom 3rem

        &.double-footer
          bottom 6rem

      &.boxed-center
        left 50%
        transform translateX(-50%)

      &.boxed-center-block
        display block
        margin 0 auto

      &.boxed-radius-20
        border-radius 20px

    for $num in range(0rem, 12rem, 1rem)
      .px-{$num}
        padding-left: $num
        padding-right: $num
      .py-{$num}
        padding-top: $num
        padding-bottom: $num
      .pl-{$num}
        padding-left: $num
      .pr-{$num}
        padding-right: $num
      .pt-{$num}
        padding-top: $num
      .pb-{$num}
        padding-bottom: $num
      .mt-{$num}
        margin-top: $num
      .mb-{$num}
        margin-bottom: $num


    .page
      position absolute
      left 0
      top 0
      width 100%
      height 100%
      padding 3rem 5rem
      background-color #ccc

      &.page-01
        background url('../assets/images/px/bg/bg01.png')
        background-size cover
        background-position left top
        background-repeat no-repeat

        .linha-separadora
          position absolute
          width 2px
          height 100%
          left calc(50% - 1px)
          top 0
          background #ccc

        .btn-left
          position absolute
          left calc(50% - 20px)
          top 50%
          transform-origin center
          transform translateX(-100%) translateY(-50%)

        .btn-right
          position absolute
          left calc(50% + 20px)
          top 50%
          transform-origin center
          transform translateX(0%) translateY(-50%)

        .btn-close-left
          position absolute
          left calc(65vw + 20px)
          top 50%
          transform-origin center
          transform translateX(0) translateY(-50%)

        .btn-close-right
          position absolute
          right calc(65vw + 20px)
          top 50%
          transform-origin center
          transform translateX(0) translateY(-50%)

        .pager-content
          &.ng-hide-add
            animation 0.5s doNothing

          .pager-left-content
            position absolute
            left 0
            top 0
            width 65%
            height 100%
            background url('../assets/images/px/formas/box_01.png')
            background-size cover
            background-position center
            background-repeat no-repeat

            &.ng-hide-add
              animation 0.5s closeToLeft
            &.ng-hide-remove
              animation 0.5s openFromLeft

          .pager-right-content
            position absolute
            right 0
            top 0
            width 65%
            height 100%
            background white
            background url('../assets/images/px/formas/box_02.png')
            background-size cover
            background-position center
            background-repeat no-repeat

            &.ng-hide-add
              animation 0.5s closeToRight
            &.ng-hide-remove
              animation 0.5s openFromRight

          .pager-content-content
            padding 6rem

      &.page-02
        background url('../assets/images/px/bg/bg02.png')
        background-size cover
        background-position center
        background-repeat no-repeat

        .principles
          line-height 120%
          width 50%

          .principle
            .principle-title
              width 100%
              background white
              border-radius 20px 20px 0 0
              padding 2rem

              p
                font-size 1.2em

            .principle-content
              width 100%
              background #eee
              border-radius 0 0 20px 20px
              padding 2rem

              p
                margin-bottom 1em
                font-weight 200


        .principles-navigator
          position absolute
          bottom 6.5rem
          display block

          .principle-sel
            display inline-block
            vertical-align top
            margin-right -4%
            width 5vw
            min-width 40px
            height 5vw
            min-height 40px
            border-radius 50%
            background linear-gradient(90deg, #012f5e, #094cab)
            line-height 5vw
            text-align center
            font-size 30px
            color white
            font-weight bold

            &:hover
              background linear-gradient(90deg, #012f5e, #012f5e)

            &.visited
              background linear-gradient(90deg, #f8941f, #ffdd03)

              &:hover
                background linear-gradient(90deg, #f8941f, #ffdd03)

            &.current
              background #522e91

              &:hover
                background #522e91

      &.page-03
        background url('../assets/images/px/bg/bg03.png')
        background-size cover
        background-position center
        background-repeat no-repeat

      &.page-04
        background url('../assets/images/px/bg/bg04.png')
        background-size cover
        background-position center
        background-repeat no-repeat

      &.page-05
        background url('../assets/images/px/bg/bg05.png')
        background-size cover
        background-position center
        background-repeat no-repeat

      &.page-06
        background url('../assets/images/px/bg/bg06.png')
        background-size cover
        background-position center
        background-repeat no-repeat

        .subpage
          position absolute
          left 0
          top 0
          width 100%
          height 100%
          padding 3rem 5rem

          .slick
            position relative
            height 100%
            width auto

            .slick-prev
              display none !important

              &.slick-disabled
                display none !important

            .slick-next
              display none !important

              &.slick-disabled
                display none !important
                animation none

            .slick-list

              .slick-track

                .slick-slide
                  position relative
                  //min-height 120px

                  img
                    position relative
                    margin 0 auto
                    width 100%

                  .content-slick
                    position absolute
                    left 0
                    top 0
                    display table
                    width 100%
                    height 100%
                    padding 6%
                    padding-bottom 11%

                    p
                      display table-cell
                      vertical-align middle
                      padding-left 1em
                      padding-right 1.5em


            ul.slick-dots
              position relative
              width 100%
              text-align center
              margin-top 10px
              padding 0
              padding-right 30px

              li
                position relative
                display inline-block
                vertical-align middle
                cursor pointer
                width 25px
                height 25px
                margin 0 5px
                border none
                background transparent

                button
                  display none

                &:after
                  content ""
                  position absolute
                  left 0px
                  top 0px
                  width 25px
                  height 25px
                  border-radius 50%
                  border 2px solid #fff
                  background #013469
                  //box-shadow 1px 1px 3px 1px rgba(0,0,0,0.7)

                &.slick-active

                  &:after
                    background #9d3bba
                    left 0
                    top 0
                    width 25px
                    height 25px

            &.smt-60
              ul.slick-dots
                margin-top -60px

          .slick-container
            position relative
            left 0
            top 0
            width calc(1.4 * 60vh)
            max-width 75vw
            height auto
            margin 0 auto

            &.subpage3
              .slick
                .slick-prev
                  display block !important
                  font-size 0
                  border none
                  outline none
                  background url(../assets/images/px/botoes/seta_voltar.png)
                  background-size cover
                  position absolute
                  left -100px
                  top calc(50% - 40px)
                  width 80px
                  height 80px
                  z-index 10

                  &.slick-disabled
                    display none !important

                .slick-next
                  display block !important
                  font-size 0
                  border none
                  outline none
                  background url(../assets/images/px/botoes/seta_avancar.png)
                  background-size cover
                  position absolute
                  right -100px
                  top calc(50% - 40px)
                  width 80px
                  height 80px
                  z-index 10

                  &.slick-disabled
                    display none !important
                    animation none

            &.subpage10
              position relative
              margin-top 15px
              left 0px
              width calc(100% - 100px)
              max-width none
              height auto

              .slick
                .slick-list .slick-track .slick-slide

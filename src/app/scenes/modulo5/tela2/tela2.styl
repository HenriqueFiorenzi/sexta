                  position absolute
                  right -45px
                  top calc(50% - 30px)
                  width 45px
                  height 45px
                  z-index 10

                  &.slick-disabled
                    display none !important
                    animation none

            &.subpage16
              position relative
              top 0px
              width 100%
              max-width auto

              .slick-list

                .slick-track

                  .slick-slide

                      p
                        font-size 1rem

          &.subpage-02
            background url('../assets/images/px/bg/bg07.png')
            background-size cover
            background-position center
            background-repeat no-repeat

          &.subpage-03
            background white

          &.subpage-04
            background url('../assets/images/px/bg/bg09.png')
            background-size cover
            background-position center
            background-repeat no-repeat

          &.subpage-05, &.subpage-06, &.subpage-07, &.subpage-08, &.subpage-09, &.subpage-10, &.subpage-11, &.subpage-12, &.subpage-13, &.subpage-14, &.subpage-15, &.subpage-16

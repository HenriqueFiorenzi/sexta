  0%
    transform translateY(0vh)
  100%
    transform translateY(100vh)

@keyframes doNothing
  0%
    opacity 1
  100%
    opacity 1


@keyframes openFromLeft
  0%
    transform translateX(-100%)
  100%
    transform translateX(0)

@keyframes closeToLeft
  0%
    transform translateX(0)
  100%
    transform translateX(-100%)

@keyframes openFromRight
  0%
    transform translateX(100%)
  100%
    transform translateX(0)

@keyframes closeToRight
  0%
    transform translateX(0)
  100%
    transform translateX(100%)

@keyframes fadeOut
  0%
    opacity 1
  100%
    opacity 0

@keyframes fadeIn
  0%
    opacity 0
  100%
    opacity 1

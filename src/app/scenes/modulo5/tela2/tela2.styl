                ul.slick-dots
                  margin-top 5px

                .slick-prev
                  display block !important
                  font-size 0
                  border none
                  outline none
                  background url(../assets/images/px/botoes/seta_voltar.png)
                  background-size cover
                  position absolute
                  left -40px
                  top calc(50% - 30px)
                  width 45px
                  height 45px
                  z-index 10

                  &.slick-disabled
                    display none !important

                .slick-next
                  display block !important
                  font-size 0
                  border none
                  outline none
                  background url(../assets/images/px/botoes/seta_avancar.png)

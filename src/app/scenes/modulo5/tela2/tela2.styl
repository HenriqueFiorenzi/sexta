            &.current
              background #522e91

              &:hover
                background #522e91

      &.page-07
        background url('../assets/images/px/bg/bg11.png')
        background-size cover
        background-position center
        background-repeat no-repeat

    &.scrollDown
      .page
        &.ng-hide-remove
          z-index 100
          animation 0.5s entraDeBaixo

        &.ng-hide-add
          animation 0.5s doNothing

    &.scrollUp
      .page
        &.ng-hide-add
          z-index 100
          animation 0.5s saiPraCima

        &.ng-hide-remove
          animation 0.5s doNothing



                  margin-bottom 1rem
                  line-height 120%

                p.subtitle
                  text-align center

                .close-btn
                  position absolute
                  right 10px
                  top 10px

                  i
                    font-size 1.5rem
                    color white

          &.subpage-10
            .img-with-text
              position relative
              margin 0 3rem

              .text
                position absolute
                left 0
                top 0
                width 64%
                //padding 2rem 3rem
                height 100%
                //overflow hidden

                p
                  //position relative
                  //left 10px
                  //top 0
                  //width 100%
                  //height 35%
                  //padding 1rem 2rem
                  //line-height 120%
                  display block
                  line-height 115%

                  &.strong
                    height auto
                    padding 0

                ul
                  display list-item
                  list-style disc !important
                  margin-left 20px

                  li
                    margin-bottom 4px


          &.subpage-16

            .img-with-text
              position relative
              margin 0 3rem

              .text
                position absolute
                left 0
                top 0
                width 57%
                //padding 2rem 3rem
                height 100%
                //overflow hidden

                p
                  position relative
                  left 10px
                  top 0
                  width 100%
                  height 35%
                  padding 1rem 2rem
                  line-height 120%

                  &.strong
                    height auto
                    padding 0

                ul
                  list-style disc

                  li
                    font-weight 200
                    line-height 120%
                    margin-left 20px

        .subpage-navigation
          position absolute
          bottom 2rem
          display block
          text-align center

          .subpage-nav
            display inline-block
            vertical-align top
            margin-right -1%
            width 5vw
            min-width 40px
            height 5vw
            min-height 40px
            border-radius 50%

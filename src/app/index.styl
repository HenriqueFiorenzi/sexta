  .pop
    position fixed !important
    left 0
    top 0
    width 100%
    height 100%
    background rgba(0,0,0,0.7) !important
    z-index 3000

    .pop-content
      position absolute
      left 0
      width 100%
      top 10%
      height calc(90% - 10px)
      text-align center

      img
        max-width 80%
        max-height 100%
        display inline-block
        vertical-align middle

        &.m-60
          max-width 55%
        &.m-40
          max-width 25%

        &.with-title
          max-height calc(100% - 3rem)

      .pop-title
        display inline-block
        width 10rem
        vertical-align middle
        margin-right 5px

      .pop-text
        display inline-block
        vertical-align middle
        width calc(100% - 11rem)
        text-align left
        margin-bottom 0

      &.overflow
        overflow hidden
        overflow-y auto

        img
          width 90%
          height auto
          max-height none


    .pop-close
      position absolute
      top 10px
      right 10px
      display block


